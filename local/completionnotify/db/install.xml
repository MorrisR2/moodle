<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/completionnotify/db" VERSION="20120808" COMMENT="XMLDB file for completionnotify module"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="completionnotify"  COMMENT="notify subscribed users (teachers?) when a student completes a course">
        <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="16" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="16" NOTNULL="true" SEQUENCE="false" PREVIOUS="courseid"/>
    </FIELDS>
    <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
    </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
