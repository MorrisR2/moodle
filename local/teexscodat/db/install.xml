<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/teexscodat/db" VERSION="20121010" COMMENT="XMLDB file for Moodle local/teexscodat"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_teexscodat_course_sess" COMMENT="Local data about course session time and completion progress" NEXT="local_teexscodat_modul_sess">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="was_completed"/>
        <FIELD NAME="was_completed" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="pct_completed"/>
        <FIELD NAME="pct_completed" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="was_completed" NEXT="session_start"/>
        <FIELD NAME="session_start" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="pct_completed" NEXT="session_seconds"/>
        <FIELD NAME="session_seconds" TYPE="int" LENGTH="6" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="session_start" NEXT="course_seconds"/>
        <FIELD NAME="course_seconds" TYPE="int" LENGTH="8" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="session_seconds" NEXT="username"/>
        <FIELD NAME="username" TYPE="char" LENGTH="64" NOTNULL="false" SEQUENCE="false" PREVIOUS="course_seconds" NEXT="course_teexid"/>
        <FIELD NAME="course_teexid" TYPE="char" LENGTH="64" NOTNULL="false" SEQUENCE="false" PREVIOUS="username" NEXT="location_data"/>
        <FIELD NAME="location_data" TYPE="char" LENGTH="128" NOTNULL="false" SEQUENCE="false" PREVIOUS="course_teexid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_teexscodat_modul_sess" COMMENT="Local TEEX completion and timing information for modules within Scorms" PREVIOUS="local_teexscodat_course_sess" NEXT="local_teexscodat_quiz_sess">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="completed"/>
        <FIELD NAME="completed" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="pct_completed"/>
        <FIELD NAME="pct_completed" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="completed" NEXT="loc"/>
        <FIELD NAME="loc" TYPE="char" LENGTH="1024" NOTNULL="false" SEQUENCE="false" PREVIOUS="pct_completed" NEXT="furthest_page"/>
        <FIELD NAME="furthest_page" TYPE="char" LENGTH="64" NOTNULL="false" SEQUENCE="false" PREVIOUS="loc" NEXT="time_in_module"/>
        <FIELD NAME="time_in_module" TYPE="int" LENGTH="8" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="furthest_page" NEXT="course_teexid"/>
        <FIELD NAME="course_teexid" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" PREVIOUS="time_in_module" NEXT="username"/>
        <FIELD NAME="username" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="course_teexid" NEXT="module_id"/>
        <FIELD NAME="module_id" TYPE="int" LENGTH="12" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="username"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="user_course_module"/>
        <KEY NAME="user_course_module" TYPE="unique" FIELDS="username, course_teexid, module_id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_teexscodat_quiz_sess" COMMENT="Local TEEX tracking about attempts of quizzes which are embedded in SCORMS" PREVIOUS="local_teexscodat_modul_sess">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="16" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="username"/>
        <FIELD NAME="username" TYPE="char" LENGTH="256" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="module_id"/>
        <FIELD NAME="module_id" TYPE="char" LENGTH="256" NOTNULL="true" SEQUENCE="false" PREVIOUS="username" NEXT="attempt_seq"/>
        <FIELD NAME="attempt_seq" TYPE="int" LENGTH="6" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="module_id" NEXT="score"/>
        <FIELD NAME="score" TYPE="int" LENGTH="3" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="attempt_seq" NEXT="xml_data"/>
        <FIELD NAME="xml_data" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="score" NEXT="timedate"/>
        <FIELD NAME="timedate" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1349902430" SEQUENCE="false" PREVIOUS="xml_data" NEXT="course_teexid"/>
        <FIELD NAME="course_teexid" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="timedate"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="id" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>